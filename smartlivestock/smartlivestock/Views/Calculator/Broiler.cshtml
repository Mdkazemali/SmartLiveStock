                                        document.getElementById("FullFatsoyabin_in").value = FullFatsoyabin_in;
                                    }

                                    var DCPMCP_in = document.getElementById("DCPMCP_in").value;

                                    if (DCPMCP_in === "") {
                                        DCPMCP_in = 0.4;
                                        document.getElementById("DCPMCP_in").value = DCPMCP_in;
                                    }
                                    var Limestone_in = document.getElementById("Limestone_in").value;
                                    if (Limestone_in === "") {
                                        Limestone_in = 1;
                                        document.getElementById("Limestone_in").value = Limestone_in;
                                    }

