                            <script>


                                function moveToNextInput(event, nextInputId) {
                                    if (event.key === 'Enter') {
                                        event.preventDefault();
                                        const nextInput = document.getElementById(nextInputId);

                                        if (nextInput) {
                                            nextInput.select();
                                        }
                                    }
                                }

                                function PrintBroilerS(event, nextInputId) {
                                    if (event.key === 'Enter') {
                                        event.preventDefault();
                                        const nextInput = document.getElementById(nextInputId);

                                        if (nextInput) {
                                            nextInput.focus();
                                        }
                                    }
                                }



                                // Function to calculate values
                                function calculateValues() {

                                    // Get the user input
                                    var Maize_in = document.getElementById("Maize_in").value;

                                    // If user doesn't input, set default value
                                    if (Maize_in === "") {
                                        Maize_in = 56;
                                        document.getElementById("Maize_in").value = Maize_in;
                                    }

                                    var Soyabinmeal_in = document.getElementById("Soyabinmeal_in").value;
