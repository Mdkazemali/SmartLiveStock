@model IEnumerable<smartlivestock.Models.Registration>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PtnId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Phone)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Gender)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Ages)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CreateDAte)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.UsrName)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PtnId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Phone)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Gender)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Ages)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CreateDAte)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UsrName)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.RegiId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.RegiId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.RegiId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
