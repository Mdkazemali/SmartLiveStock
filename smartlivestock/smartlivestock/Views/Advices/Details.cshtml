@model smartlivestock.Models.Advice

@{
    ViewData["Title"] = "Details";
}


<div>
    <h4>Advices Informations</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            Advices
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.AdvName)
        </dd>
        <dt class = "col-sm-2">
            Advices Date
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.AdvDate)
        </dd>
        <dt class = "col-sm-2">
           User Name
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.UrName)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" class="btn btn-primary" asp-route-id="@Model?.AdvId">Edit</a>
    <a asp-action="Index" class="btn btn-primary">Back</a>
</div>
