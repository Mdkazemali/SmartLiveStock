@model IEnumerable<smartlivestock.Models.Invastigation>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.InvName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.InvDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.UrName)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.InvName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.InvDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UrName)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.InvId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.InvId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.InvId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
